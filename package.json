{
  "name": "cdn",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && sass --no-source-map src/sass:dist/css",
    "build:ts": "tsc",
    "build:sass": "sass --no-source-map -s compressed src/sass:dist/css && html-minifier -c html-min.config.json --input-dir dist/css --output-dir css/css",
    "watch:ts":"tsc -w",
    "watch:sass":"sass -w --no-source-map src/sass:dist/css",
    "watch:sass-video":"sass -w --no-source-map src/sass/video:dist/css/video",
    "min:html": "html-minifier -c html-min.config.json index.html -o dist/index.min.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "html-minifier": "^4.0.0",
    "sass": "^1.63.2",
    "typescript": "^5.1.3",
    "uglifyjs-folder": "^3.2.0"
  },
  "optionalDependencies": {
    "node-sass": "^9.0.0"
  }
}
